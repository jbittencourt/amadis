<cmapp>
<cmdevel>
   <smartform>
     <calendar>
         <images main="bt_calendar.png" left="bt_cal_prev.gif" right="bt_cal_next.gif" />
     </calendar>
   </smartform>
</cmdevel>
<app>
   <general>
       <admin_email>admin@yourdomain.org</admin_email>
       <version>1.3.1</version>
   </general>
   <libs>
      <lib name="cmdevel" path="COMPLETE_WITH_YOUR_PATH/cmdevel" />
      <lib name="xoad" path="COMPLETE_WITH_YOUR_PATH/environment/media/libs/xoad" />
      <lib name="lastRSS" path="COMPLETE_WITH_YOUR_PATH/environment/media/libs/lastRSS" />
   </libs>
   <environment>
      <plataform>1</plataform>
      <plataform_name>AMADIS</plataform_name>
      <use_md5_password>1</use_md5_password>
      <store_plain_password>0</store_plain_password>
      <auto_accept_subscribe>0</auto_accept_subscribe>
      <use_complete_register_form>0</use_complete_register_form>
      <debug_mode>0</debug_mode>
   </environment>
   <session>
      <name>AMADIS</name>
      <timeout>1500</timeout>
   </session>
   <urls>
      <base>COMPLETE_WITH_YOUR_URL</base>
      <media>COMPLETE_WITH_YOUR_URL/media</media>
      <images>COMPLETE_WITH_YOUR_URL/media/images</images>
      <js>COMPLETE_WITH_YOUR_URL/media/js</js>
      <css>COMPLETE_WITH_YOUR_URL/media/css</css>
      <thumbnails>COMPLETE_WITH_YOUR_URL/media/thumbnails</thumbnails>
      <services>COMPLETE_WITH_YOUR_URL/tools</services>
      <pages>COMPLETE_WITH_YOUR_URL/paginas</pages>
   </urls>
   <paths>
      <pages>COMPLETE_WITH_YOUR_PATH/environment/paginas</pages>
      <thumbnails>COMPLETE_WITH_YOUR_PATH/environment/media/thumbnails</thumbnails>
      <cache>COMPLETE_YOUR_PATH/cache</cache>
	  <log>COMPLETE_YOUR_PATH/log</log>
	  <files>COMPLETE_YOUR_PATH/environment/files</files>
   </paths>
   <database>
      <driver>mysql</driver>
      <host>DB_HOST</host>
      <user>DB_USER</user>
      <password>DB_PASSWORD</password>
      <name>DB_NAME</name>
   </database>
   <userclass>
      <class>AMUser</class>
      <lib>amuser.inc.php</lib>
   </userclass>
   <languages>
      <active>1</active>
      <default>pt-br</default>
      <language name="Portugues Brasil" file="pt-br.lang" prefix="pt-br" />
      <language name="Espanhol" file="es-uy.lang" prefix="es-uy" />
      <language name="English" file="en.lang" prefix="en" />
   </languages>
   <tools>
      <finder>
         <timeout>300</timeout>
         <window_refresh_timeout>30</window_refresh_timeout>
         <activate_emotions>1</activate_emotions>
     </finder>
   </tools>
